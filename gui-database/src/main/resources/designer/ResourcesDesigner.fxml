<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" prefHeight="768.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.tduf.gui.database.controllers.ResourcesStageController">
   <children>
      <VBox id="mainVbox" prefWidth="768.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <ToolBar prefHeight="40.0" prefWidth="768.0" styleClass="navigationToolbar">
               <items>
                  <ChoiceBox fx:id="topicsChoiceBox" prefWidth="200.0" />
                  <Separator orientation="VERTICAL" prefHeight="30.0" />
                  <Button fx:id="addResourceButton" layoutX="224.0" layoutY="13.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="28.0" text="+" />
                  <Button fx:id="removeResourceButton" layoutX="297.0" layoutY="13.0" mnemonicParsing="false" prefWidth="28.0" text="-" />
                  <Separator layoutX="214.0" layoutY="10.0" orientation="VERTICAL" prefHeight="30.0" />
                  <AnchorPane prefHeight="30.0" prefWidth="334.0">
                     <children>
                        <Button fx:id="selectResourceButton" layoutY="3.0" mnemonicParsing="false" text="Select" AnchorPane.rightAnchor="0.0" />
                     </children>
                  </AnchorPane>
               </items>
            </ToolBar>
            <Pane prefHeight="744.0" prefWidth="768.0">
               <children>
                  <TableView fx:id="resourcesTableView" onMouseClicked="#handleResourceTableMouseClick" prefHeight="744.0" prefWidth="640.0">
                    <columns>
                      <TableColumn prefWidth="96.0" text="#" />
                      <TableColumn prefWidth="543.0" text="Resource value" />
                    </columns>
                  </TableView>
               </children>
            </Pane>
         </children></VBox>
   </children>
</AnchorPane>
