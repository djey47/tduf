<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" prefHeight="768.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.tduf.gui.database.controllers.EntriesStageController">
   <children>
      <VBox id="mainVbox" prefWidth="768.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <ToolBar prefHeight="40.0" prefWidth="768.0" styleClass="navigationToolbar">
               <items>
                  <AnchorPane prefHeight="30.0" prefWidth="626.0">
                     <children>
                        <Label fx:id="currentTopicLabel" alignment="TOP_CENTER" layoutY="7.0" prefWidth="175.0" text="&lt;TOPIC&gt;" AnchorPane.leftAnchor="0.0">
                           <font>
                              <Font name="System Bold Italic" size="13.0" />
                           </font>
                        </Label>
                        <Button layoutY="3.0" mnemonicParsing="false" onAction="#handleSelectEntryButtonMouseClick" text="Select" AnchorPane.rightAnchor="0.0" />
                     </children>
                  </AnchorPane>
               </items>
            </ToolBar>
            <Pane prefHeight="744.0" prefWidth="768.0">
               <children>
                  <TableView fx:id="entriesTableView" onMouseClicked="#handleResourceTableMouseClick" prefHeight="744.0" prefWidth="640.0">
                    <columns>
                      <TableColumn prefWidth="96.0" text="REF" />
                      <TableColumn prefWidth="543.0" text="Value" />
                    </columns>
                  </TableView>
               </children>
            </Pane>
         </children></VBox>
   </children>
</AnchorPane>
