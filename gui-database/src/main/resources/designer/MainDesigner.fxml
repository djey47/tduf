<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<AnchorPane fx:id="root" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.tduf.gui.database.controllers.MainStageController">
   <children>
      <VBox id="mainVbox" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <children>
              <ToolBar prefHeight="40.0" prefWidth="1280.0" styleClass="locationToolbar" VBox.vgrow="NEVER">
                  <items>
                      <Label text="TDU database:">
                          <font>
                              <Font name="System Bold" size="13.0" />
                          </font>
                      </Label>
                      <TextField fx:id="databaseLocationTextField" />
                      <Button mnemonicParsing="false" onAction="#handleBrowseDirectoryButtonMouseClick" prefWidth="34.0" text="..."><tooltip><Tooltip text="Selects a directory for database JSON files." /></tooltip></Button>
                      <Button mnemonicParsing="false" onAction="#handleLoadButtonMouseClick" prefWidth="60.0" text="Load"><tooltip><Tooltip text="Opens database JSON files from selected directory." /></tooltip></Button>
                      <Button layoutX="10.0" layoutY="11.0" mnemonicParsing="false" onAction="#handleSaveButtonMouseClick" prefWidth="60.0" text="Save"><tooltip><Tooltip text="Saves current database to JSON files in selected directory." /></tooltip></Button>
                  </items>
              </ToolBar>
      
              <TitledPane fx:id="settingsPane" prefWidth="1280.0" styleClass="settingsPane" text="Settings" VBox.vgrow="SOMETIMES">
                  <content>
                      <AnchorPane styleClass="settingsPane">
                          <children>
                              <ToolBar styleClass="settingsToolbar" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                  <items>
                                      <Label text="Profile:">
                                 <font>
                                    <Font name="System Bold" size="13.0" />
                                 </font></Label>
                                      <ChoiceBox fx:id="profilesChoiceBox" prefWidth="175.0" />
                                      <Label text="Locale:">
                                 <font>
                                    <Font name="System Bold" size="13.0" />
                                 </font></Label>
                                      <ChoiceBox fx:id="localesChoiceBox" prefWidth="175.0" />
                                  </items>
                              </ToolBar>
                          </children>
                     <padding>
                        <Insets bottom="-1.0" left="-1.0" right="-1.0" top="-1.0" />
                     </padding>
                      </AnchorPane>
                  </content>
              </TitledPane>
      
              <ToolBar prefHeight="40.0" prefWidth="200.0" styleClass="navigationToolbar" VBox.vgrow="NEVER">
                  <items>
                  <Button mnemonicParsing="false" onAction="#handleBackButtonMouseClick" prefWidth="34.0" text="&lt;-"><tooltip><Tooltip text="Goes back to previous topic location." /></tooltip></Button>
                  <Label fx:id="currentTopicLabel" alignment="TOP_CENTER" prefWidth="175.0" text="&lt;TOPIC&gt;">
                     <font>
                        <Font name="System Bold Italic" size="13.0" />
                     </font>
                  </Label>
                  <Separator orientation="VERTICAL" />
                  <Button layoutX="237.0" layoutY="12.0" mnemonicParsing="false" onAction="#handleDuplicateEntryButtonAction" prefWidth="34.0" text="*">
                     <tooltip>
                        <Tooltip text="Adds a new entry in current topic, based on displayed one." />
                     </tooltip>
                  </Button>
                  <Button layoutX="199.0" layoutY="12.0" mnemonicParsing="false" onAction="#handleAddEntryButtonAction" prefWidth="34.0" text="+"><tooltip><Tooltip text="Adds a new entry in current topic." /></tooltip></Button>
                  <Button layoutX="237.0" layoutY="12.0" mnemonicParsing="false" onAction="#handleRemoveEntryButtonAction" prefWidth="34.0" text="-"><tooltip><Tooltip text="Removes current entry in topic." /></tooltip></Button>
                  <Separator layoutX="189.0" layoutY="10.0" orientation="VERTICAL" />
                  <MenuButton layoutX="361.0" layoutY="10.0" mnemonicParsing="false" prefWidth="60.0" text="[]&lt;">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#handleImportEntryTdufPatchMenuAction" text="TDUF patch..." />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" onAction="#handleImportPerformancePackMenuAction" text="TDUPE Performance Pack..." />
                     </items>
                     <tooltip>
                        <Tooltip text="Imports to current database." />
                     </tooltip>
                  </MenuButton>
                  <MenuButton mnemonicParsing="false" prefWidth="60.0" text="[]&gt;">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#handleExportEntryTdufPatchMenuAction" text="TDUF patch..." />
                        <SeparatorMenuItem mnemonicParsing="false" />
                      <MenuItem mnemonicParsing="false" onAction="#handleExportEntryLineMenuAction" text="Line..." />
                        <MenuItem mnemonicParsing="false" onAction="#handleExportEntryPchMenuAction" text="PCH instruction..." />
                    </items>
                      <tooltip>
                          <Tooltip text="Exports one or more entries." />
                      </tooltip>
                  </MenuButton>
                  <Separator layoutX="418.0" layoutY="10.0" orientation="VERTICAL" />
                  <ComboBox fx:id="entryNumberComboBox" prefWidth="60.0" promptText="..." visibleRowCount="25"><tooltip><Tooltip text="Quick-selects an entry in current topic." /></tooltip></ComboBox>
                  <Label fx:id="currentEntryLabel" text="&lt;current&gt;">
                     <font>
                        <Font name="System Italic" size="13.0" />
                     </font>
                  </Label>
                  </items>
              </ToolBar>
                  <TabPane fx:id="tabPane" prefHeight="626.0" prefWidth="1303.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
                      <tabs>
                          <Tab text="Default">
                              <content>
                        <ScrollPane prefViewportHeight="167.0" prefViewportWidth="1303.0">
                           <content>
                                        <VBox fx:id="defaultTab" minHeight="0.0" minWidth="0.0" />
                           </content>
                        </ScrollPane>
                              </content>
                          </Tab>
                      </tabs>
                  </TabPane>
            <ToolBar layoutX="10.0" layoutY="112.0" prefHeight="40.0" prefWidth="200.0" styleClass="navigationToolbar" VBox.vgrow="NEVER">
               <items>
                  <Button mnemonicParsing="false" onAction="#handleSearchEntryButtonAction" prefWidth="40.0" text="?"><tooltip><Tooltip text="Searches entry with REF." /></tooltip></Button>
                  <Separator orientation="VERTICAL" />
                  <Button mnemonicParsing="false" onAction="#handleFirstButtonMouseClick" prefWidth="40.0" text="|&lt;"><tooltip><Tooltip text="Goes to first entry." /></tooltip></Button>
                  <Button mnemonicParsing="false" onAction="#handleFastPreviousButtonMouseClick" prefWidth="40.0" text="&lt;&lt;"><tooltip><Tooltip text="Goes back up to 10 entries." /></tooltip></Button>
                  <Button mnemonicParsing="false" onAction="#handlePreviousButtonMouseClick" prefWidth="40.0" text="&lt;"><tooltip><Tooltip text="Goes to previous entry." /></tooltip></Button>
                  <TextField fx:id="entryNumberTextField" alignment="CENTER" onKeyPressed="#handleEntryNumberTextFieldKeyPressed" prefWidth="50.0" />
                  <Label fx:id="entryItemsCountLabel" prefWidth="50.0" />
                  <Button mnemonicParsing="false" onAction="#handleNextButtonMouseClick" prefWidth="40.0" text="&gt;"><tooltip><Tooltip text="Goes to next entry." /></tooltip></Button>
                  <Button mnemonicParsing="false" onAction="#handleFastNextButtonMouseClick" prefWidth="40.0" text="&gt;&gt;"><tooltip><Tooltip text="Goes forward up to 10 entries." /></tooltip></Button>
                  <Button mnemonicParsing="false" onAction="#handleLastButtonMouseClick" prefWidth="40.0" text="&gt;|"><tooltip><Tooltip text="Goes to very last entry." /></tooltip></Button>
                  <Separator layoutX="382.0" layoutY="10.0" orientation="VERTICAL" />
                  <Label fx:id="statusLabel">
                     <font>
                        <Font name="System Italic" size="13.0" />
                     </font>
                  </Label>
               </items>
            </ToolBar>
          </children>
      </VBox>
   </children>
</AnchorPane>
